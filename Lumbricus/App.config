<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <section name="lumbricusConfig" type="TwoWholeWorms.Lumbricus.Shared.LumbricusConfiguration, LumbricusShared" />
        <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
        <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework" /> 
    </configSections>
    <connectionStrings> 
        <add name="LumbricusContext" providerName="MySql.Data.MySqlClient" connectionString="Server=127.0.01;Database=lumbricus;User ID=lumbricus;Password=lumbricus;Pooling=false" /> 
        <add name="InfoContext" providerName="MySql.Data.MySqlClient" connectionString="Server=127.0.01;Database=lumbricus;User ID=lumbricus;Password=lumbricus;Pooling=false" /> 
        <add name="MugshotsContext" providerName="MySql.Data.MySqlClient" connectionString="Server=127.0.01;Database=lumbricus;User ID=lumbricus;Password=lumbricus;Pooling=false" /> 
        <add name="IrcLogContext" providerName="MySql.Data.MySqlClient" connectionString="Server=127.0.01;Database=lumbricus;User ID=lumbricus;Password=lumbricus;Pooling=false" /> 
    </connectionStrings>
    <lumbricusConfig>
        <plugins>
            <plugin name="IrcConnectionPlugin" enabled="true">
                <settings name="freenode" type="TwoWholeWorms.Lumbricus.Plugins.IrcConnection.Model.Server">
                    <parameters>
                        <add name="Host" value="irc.freenode.net" />
                        <add name="Port" value="6667" />
                        <add name="BotNick" value="Lumbricus" />
                        <add name="BotNickPassword" value="Lumbricus" />
                        <add name="BotUserName" value="Lumbricus" />
                        <add name="BotRealName" value="Lumbricus Bot Framework" />
                        <add name="NickServNick" value="NickServ" />
                        <add name="NickServHost" value="Services" />
                        <add name="AutoConnect" value="true" />
                    </parameters>
                </settings>
            </plugin>
            <plugin name="TelegramConnectionPlugin" enabled="true">
                <settings name="telegram" type="TwoWholeWorms.Lumbricus.Plugins.TelegramConnection.Model.Server">
                    <parameters>
                        <add name="Domain" value="api.telegram.org" />
                        <add name="AccessToken" value="1234567890:ABCDEFghijklMNOPqrstUVWxyz1234567890" />
                    </parameters>
                </settings>
            </plugin>
        </plugins>
    </lumbricusConfig>
    <nlog>
        <targets>
            <target name="logfile" type="File" fileName="${basedir}/logs/Lumbricus.${date:format=yyyy-MM-dd}.log" />
            <target name="console" type="Console" error="True" layout="[${longdate}] ${pad:padding=5:inner=${level:uppercase=true}} — ${message}" />
        </targets>
        <rules>
            <logger name="*" minlevel="Info" writeTo="logfile" />
            <logger name="*" minlevel="Trace" writeTo="console" />
        </rules>
    </nlog>
    
    
<system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.9.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data><entityFramework>
    <providers>
      <provider invariantName="MySql.Data.MySqlClient" type="MySql.Data.MySqlClient.MySqlProviderServices, MySql.Data.Entity.EF6, Version=6.9.9.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d"></provider>
    </providers>
  </entityFramework></configuration>
